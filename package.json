{
  "name": "@loyalteez/react-components",
  "version": "0.0.1",
  "description": "React components for building loyalty program interfaces with Loyalteez",
  "license": "MIT",
  "author": "Loyalteez Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alpha4-Labs/loyalteez-react-components"
  },
  "homepage": "https://loyalteez.app",
  "bugs": {
    "url": "https://github.com/Alpha4-Labs/loyalteez-react-components/issues"
  },
  "keywords": [
    "react",
    "components",
    "loyalty",
    "rewards",
    "gamification",
    "loyalteez",
    "web3",
    "points",
    "tiers",
    "perks",
    "leaderboard",
    "badges"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

